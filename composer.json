{
  "name": "akabihno/lingwhaat",
  "description": "description",
  "minimum-stability": "stable",
  "license": "CC-BY-SA and GFDL",
  "authors": [
    {
      "name": "akabihno",
      "email": "arsenijs.kabihno@gmail.com"
    }
  ],
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "bump-after-update": true,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*",
    "symfony/polyfill-php82": "*"
  },
  "require": {
    "php": "^8.2",
    "vlucas/phpdotenv": ">=v5.6.1",
    "ivopetkov/html5-dom-document-php": "2.*",
    "ext-pdo": "*",
    "symfony/orm-pack": "^v2.4.1",
    "doctrine/dbal": "^3.9.4",
    "symfony/framework-bundle": "^v7.2.3",
    "symfony/http-kernel": "^v7.2.3",
    "symfony/http-foundation": "^v7.2.3",
    "symfony/routing": "^v7.2.3",
    "symfony/dependency-injection": "^v7.2.3",
    "symfony/config": "^v7.2.3",
    "symfony/dotenv": "^v7.2.0",
    "symfony/console": "^v7.2.1",
    "twig/twig": "^v3.20.0",
    "symfony/twig-bundle": "^v7.2.0"
  },
  "require-dev": {
    "symfony/maker-bundle": "^v1.62.1"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  }
}