{
  "name": "akabihno/lingwhaat",
  "description": "description",
  "minimum-stability": "stable",
  "license": "CC-BY-SA and GFDL",
  "authors": [
    {
      "name": "akabihno",
      "email": "arsenijs.kabihno@gmail.com"
    }
  ],
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "bump-after-update": true,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*",
    "symfony/polyfill-php82": "*",
    "symfony/polyfill-php84": "*"
  },
  "require": {
    "php": "^8.4",
    "ext-pdo": "*",
    "doctrine/dbal": "^3.10.3",
    "doctrine/doctrine-bundle": "^2.18.0",
    "doctrine/doctrine-migrations-bundle": "^3.4.2",
    "doctrine/orm": "^3.5.2",
    "ivopetkov/html5-dom-document-php": "^2.8.1",
    "symfony/config": "^7.3.4",
    "symfony/console": "^7.3.4",
    "symfony/dependency-injection": "^7.3.4",
    "symfony/dotenv": "^7.3.2",
    "symfony/flex": "^2.8.2",
    "symfony/framework-bundle": "^7.3.4",
    "symfony/http-foundation": "^7.3.4",
    "symfony/http-kernel": "^7.3.4",
    "symfony/http-client": "^7.3.4",
    "symfony/routing": "^7.3.4",
    "symfony/runtime": "7.2.*",
    "symfony/twig-bundle": "^7.3.4",
    "symfony/yaml": "7.2.*",
    "symfony/asset": ">=7.3.0",
    "symfony/monolog-bundle": "^v3.10.0",
    "symfony/uid": "^7.3.1",
    "symfony/rate-limiter": "v7.2.0",
    "symfony/lock": "^7.3.4",
    "twig/twig": "^3.21.1",
    "vlucas/phpdotenv": ">=5.6.2",
    "ext-mbstring": "*",
    "nelmio/api-doc-bundle": "^v5.6.3",
    "ruflin/elastica": "^9.0.0"
  },
  "require-dev": {
    "symfony/maker-bundle": "^1.64.0",
    "vimeo/psalm": "^6.13.1"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
