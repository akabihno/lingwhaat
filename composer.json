{
  "name": "akabihno/lingwhaat",
  "description": "description",
  "minimum-stability": "stable",
  "license": "CC-BY-SA and GFDL",
  "authors": [
    {
      "name": "akabihno",
      "email": "arsenijs.kabihno@gmail.com"
    }
  ],
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "bump-after-update": true,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*",
    "symfony/polyfill-php82": "*"
  },
  "require": {
    "php": "^8.2",
    "ext-pdo": "*",
    "doctrine/dbal": "^3.9.4",
    "doctrine/doctrine-bundle": "^2.14",
    "doctrine/doctrine-migrations-bundle": "^3.4.1",
    "doctrine/orm": "^3.3.3",
    "ivopetkov/html5-dom-document-php": "^2.8",
    "symfony/config": "^7.2.6",
    "symfony/console": "^7.2.6",
    "symfony/dependency-injection": "^7.2.6",
    "symfony/dotenv": "^v7.2.0",
    "symfony/flex": "^2.5",
    "symfony/framework-bundle": "^7.2.5",
    "symfony/http-foundation": "^7.2.6",
    "symfony/http-kernel": "^7.2.6",
    "symfony/http-client": "^v7.2.4",
    "symfony/routing": "^v7.2.3",
    "symfony/runtime": "7.2.*",
    "symfony/twig-bundle": "^v7.2.0",
    "symfony/yaml": "7.2.*",
    "symfony/asset": ">=v7.2.0",
    "symfony/monolog-bundle": "^v3.10.0",
    "symfony/uid": "^v7.2.0",
    "twig/twig": "^3.21.1",
    "vlucas/phpdotenv": ">=5.6.2",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "symfony/maker-bundle": "^1.63.0"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
